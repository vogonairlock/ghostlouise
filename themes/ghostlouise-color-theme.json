			"settings": {
				"foreground": "#9b556b",
				"fontStyle": ""
			}
		},
		{
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#673947"
			}
		},
		{
			"scope": [
				"meta.diff",
