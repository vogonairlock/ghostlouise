	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#9682C8"
			}
		},
		{
			"scope": "string.quoted.docstring.multi.python",
			"settings": {
				"foreground": "#9682C8"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#a987e2"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#956fd1"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#b083f7"
			}
		},
		{
			"scope": [
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#9b6e81"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"fontStyle": "bold",
                "foreground": "#9472e0"
			}
		},
		{
			"scope": "punctuation.separator.period.python,punctuation.separator.element.python,punctuation.parenthesis.begin.python,punctuation.parenthesis.end.python",
			"settings": {
                "foreground": "#9b6e81"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#b083f7"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#9B9FDE",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#b77c8e",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "entity.name.class",
			"settings": {
				"foreground": "#9472e0",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"variable.language.this"
			],
			"settings": {
				"foreground": "#9472e0"
			}
		},
		{
			"scope": "entity.other.inherited-class",
			"settings": {
				"foreground": "#9472e0",
				"fontStyle": "bold"
			}
		},
		{
