			"settings": {
				"foreground": "#9472e0",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#956fd1",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#b77c8e",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "support.function",
			"settings": {
				"foreground": "#b77c8e",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "support.constant",
			"settings": {
				"foreground": "#b083f7",
				"fontStyle": ""
			}
